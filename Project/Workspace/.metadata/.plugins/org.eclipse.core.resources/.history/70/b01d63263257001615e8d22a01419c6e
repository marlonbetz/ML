import numpy as np 
from keras.models import Model
from keras.layers import GRU,Dense,Input,Merge,Embedding

maxlen = 10
n_symbols = 1000
dim_embedding_phono = 10
dim_embedding_language = 10
dim_embedding_info = 10
n_languages  =100
n_info = 1000

#input phono
input_phono = Input((n_symbols,))
embedded_phono = Embedding(n_symbols, dim_embedding_phono, input_length=maxlen)(input_phono)
embedded_phono = GRU(output_dim=dim_embedding_phono, input_shape=(maxlen, len(n_symbols)))(input_phono)
embedded_phono = GRU(output_dim=dim_embedding_phono)(input_phono)

#input language
input_language = Input((n_languages,))
embedded_language = Dense(dim_embedding_language,activation="linear")(input_phono)

#input info
input_language = Input((1000,))
embedded_info = Dense(dim_embedding_info,activation="linear")(input_phono)


